<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	
	<!-- 监听request对象创建、销毁  -->
	<listener>
		<listener-class>com.github.jlran.a_life.Demo01</listener-class>
	</listener>
	<!-- 监听servletContext对象创建、销毁 -->
	<listener>
		<listener-class>com.github.jlran.a_life.Demo02</listener-class>
	</listener>
	<!-- 监听session对象创建、销毁 -->
	<listener>
		<listener-class>com.github.jlran.a_life.Demo03</listener-class>
	</listener>
	<!-- 属性监听器 -->
	<listener>
		<listener-class>com.github.jlran.a_life.Demo04</listener-class>
	</listener>
	
  <display-name></display-name>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
